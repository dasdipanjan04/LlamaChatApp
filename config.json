{
  "tokenizer": {
    "return_tensors": "pt",
    "truncation": true,
    "max_length": 2048,
    "padding": true,
    "return_attention_mask": true
  },
  "streamer": {
    "timeout": 60.0,
    "skip_special_tokens": true,
    "skip_prompt": true
  },
  "generate_kwargs": {
    "max_length": 500,
    "min_length": 10,
    "length_penalty": 0.5,
    "do_sample": true,
    "top_p": 0.4,
    "top_k": 100,
    "temperature": 0.8,
    "repetition_penalty": 1.2,
    "no_repeat_ngram_size": 3,
    "early_stopping": true
  },
  "stopping_criteria": {
    "num_sentences": 10000
  }
}
